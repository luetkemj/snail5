    "scribe-rpg-coin-purse": "1.2.4",
    "wrap-ansi": "6.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "babel-plugin-transform-default-import": "1.0.0",
    "cssnano": "4.1.10",
